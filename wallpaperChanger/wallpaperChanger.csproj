<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <AssemblyName>WallpaperChanger</AssemblyName>
    <Description>Changes your wallpaper at certain times throughout the day.</Description>
    <Copyright>hummusbird</Copyright>
    <PackageProjectUrl>https://www.hummusbird.co.uk/projects/wallpaperchanger</PackageProjectUrl>
    <Version>1.2</Version>
    <PackageReleaseNotes>Fixes an issue that occurs when the wallpaper is changed between 12 and 1</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Win32.Registry" Version="5.0.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

</Project>
